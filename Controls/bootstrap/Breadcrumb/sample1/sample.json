{
  "files": [
    {
      "name": "page.dothtml",
      "registerRoute": true,
      "routeName": "MyRoute",
      "routeUrl": "{Id?}",
      "replacements": [
        {
          "pattern": "RouteName=\"(MyRoute)\"",
          "replacement": "%SampleUniqueName%_MyRoute"
        },
        {
          "pattern": "(https?://)",
          "replacement": "../../?url=\\1"
        }
      ]
    },
    {
      "name": "ViewModel.cs"
    },
    {
      "name": "DotvvmStartup.cs",
      "doNotExport": true
    }
  ]
}