<h3>IsJustified = End</h3>
<bs:NavBar Justify="End" Placement="FixedTop" backgroundColor="Primary">
    <BrandTemplate>
        Brand Name
    </BrandTemplate>
    <bs:NavBarSection>
        <bs:NavBarLink Click="{staticCommand: null}">
            item
        </bs:NavBarLink>
        <bs:NavBarDropDown HeaderText="DropDownMenu">
            <bs:DropDownItem>
                item
            </bs:DropDownItem>
        </bs:NavBarDropDown>
    </bs:NavBarSection>
</bs:NavBar>

<bs:NavBar Justify="Center" Placement="FixedBottom" backgroundColor="Primary">
    <BrandTemplate>
        Brand Name
    </BrandTemplate>
    <bs:NavBarSection>
        <bs:NavBarLink Click="{staticCommand: null}">
            item
        </bs:NavBarLink>
        <bs:NavBarDropDown HeaderText="DropDownMenu">
            <bs:DropDownItem>
                item
            </bs:DropDownItem>
        </bs:NavBarDropDown>
    </bs:NavBarSection>
</bs:NavBar>