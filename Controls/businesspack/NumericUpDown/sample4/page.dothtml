<bp:NumericUpDown Value="{value: Value}"
                  MinValue="0.5"
                  MaxValue="5"
                  Step="0.5">
    <IncreaseIcon>
        <bp:Icon Icon="Plus"></bp:Icon>
    </IncreaseIcon>
    <DecreaseIcon>
        <bp:Icon Icon="Minus"></bp:Icon>
    </DecreaseIcon>
</bp:NumericUpDown>